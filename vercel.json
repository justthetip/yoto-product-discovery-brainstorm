{
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/web/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/(favicon\\.svg|site\\.webmanifest)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/ai-chat",
      "destination": "/web/layout6-ai-chat.html"
    },
    {
      "source": "/favicon.svg",
      "destination": "/public/favicon.svg"
    },
    {
      "source": "/site.webmanifest",
      "destination": "/public/site.webmanifest"
    }
  ]
}
