{
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/web/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/(favicon\\.svg|site\\.webmanifest)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/ai-chat",
      "destination": "/web/layout6-ai-chat.html"
    },
    {
      "source": "/designs",
      "destination": "/web/ai-design-explorer.html"
    },
    {
      "source": "/ai-v1-elevated",
      "destination": "/web/ai-v1-elevated.html"
    },
    {
      "source": "/ai-v2-soft",
      "destination": "/web/ai-v2-soft.html"
    },
    {
      "source": "/ai-v3-bold",
      "destination": "/web/ai-v3-bold.html"
    },
    {
      "source": "/ai-v4-minimal",
      "destination": "/web/ai-v4-minimal.html"
    },
    {
      "source": "/ai-v5-compact",
      "destination": "/web/ai-v5-compact.html"
    },
    {
      "source": "/ai-v5-layouts",
      "destination": "/web/ai-v5-layouts.html"
    },
    {
      "source": "/ai-v5-layout-a",
      "destination": "/web/ai-v5-layout-a.html"
    },
    {
      "source": "/ai-v5-layout-b",
      "destination": "/web/ai-v5-layout-b.html"
    },
    {
      "source": "/ai-v5-layout-c",
      "destination": "/web/ai-v5-layout-c.html"
    },
    {
      "source": "/favicon.svg",
      "destination": "/public/favicon.svg"
    },
    {
      "source": "/site.webmanifest",
      "destination": "/public/site.webmanifest"
    }
  ]
}
